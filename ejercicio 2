using System;
					
public class Program
{
	public static void Main()
	{
		int [] numsfavoritos = new int [5];
		numsfavoritos[0] = 7;
		numsfavoritos[1] = 5;
		numsfavoritos[2] = 3;
		numsfavoritos[3] = 11;
		numsfavoritos[4] = 13;
		
		for (int i = 0; i < 5 ; i++)
		{
		Console.WriteLine("Tu " + (i+1) + "º número favorito es: " + numsfavoritos[i]);
	  }

	}
}

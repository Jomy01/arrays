using System;
					
public class Program
{
	public static void Main()
	{
		int [] numsfavoritos = new int [5];
		for (int i = 0; i < 5; i++)
	{
	Console.WriteLine("Introduzca el elemento " + i);
	numsfavoritos[i] = Convert.ToInt32(Console.ReadLine());
	}

		
		for (int i = 0; i < 5 ; i++)
			 {
		Console.WriteLine("Tus nÃºmeros favoritos son: " + numsfavoritos[i]);
	}
 
	}
}
